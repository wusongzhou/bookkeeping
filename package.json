{
  "name": "bookkeeping",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "next dev",
    "dev:electron": "concurrently \"cross-env NODE_ENV=development next dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "build": "next build",
    "build:electron": "tsc -p tsconfig.electron.json && next build && electron-builder",
    "start": "next start",
    "lint": "eslint",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "^13.0.0",
    "wait-on": "^9.0.3",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
